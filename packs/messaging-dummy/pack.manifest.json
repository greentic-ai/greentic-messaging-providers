{
  "name": "messaging-dummy",
  "version": "0.0.0",
  "description": "Deterministic provider-core messaging provider for CI and integration tests.",
  "kind": "application",
  "publisher": "Greentic",
  "config_schema": {
    "provider_config": {
      "format": "json",
      "path": "schemas/messaging/dummy/config.schema.json"
    }
  },
  "components": [
    "messaging-provider-dummy"
  ],
  "extensions": {
    "greentic.ext.provider": {
      "kind": "greentic.ext.provider",
      "version": "0.0.0",
      "inline": {
        "providers": [
          {
            "kind": "messaging",
            "provider_type": "messaging.dummy",
            "capabilities": [
              "messaging"
            ],
            "ops": [
              "send"
            ],
            "config_schema_ref": "schemas/messaging/dummy/config.schema.json",
            "state_schema_ref": "",
            "runtime": {
              "component_ref": "messaging-provider-dummy",
              "export": "schema-core-api",
              "world": "greentic:provider/schema-core@1.0.0"
            },
            "docs_ref": ""
          }
        ]
      }
    }
  },
  "secret_requirements": []
}
